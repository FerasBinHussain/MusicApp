<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ferasbinhussainngmail.musicapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".EnriqueActivity"
            android:label="Enrique Iglesias" />
        <activity
            android:name=".SiaActivity"
            android:label="Sia" />
        <activity
            android:name=".TalalActivity"
            android:label="Talal Maddah" />
        <activity
            android:name=".AdeleActivity"
            android:label="Adele" />
        <activity
            android:name=".EnriqueMedia"
            android:label="@string/title_activity_enrique_media"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".SiaMedia" />
        <activity android:name=".TalalMedia" />
        <activity android:name=".AdeleMedia"></activity>
    </application>

</manifest>